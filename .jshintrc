{
	"bitwise" 		: true,
	"camelcase" 	: true,
	"curly" 		: true,
	"eqeqeq" 		: true,
	"forin" 		: true,
	"freeze" 		: true,
	"immed" 		: true,
	"indent" 		: 4,
	"latedef" 		: true,
	"noarg" 		: true,
	"noempty"		: true,
	"nonbsp"		: true,
	"nonew"			: true,
